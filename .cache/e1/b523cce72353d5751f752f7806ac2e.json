lue = path.split('/')[path.split('/').length - 1];\n            let key = this.currentRoute.path\n                .split('/')[this.currentRoute.path.split('/').length - 1]\n                    .split('')\n                    .splice(1)\n                    .join('');\n            data[key] = value;\n            this.currentRoute.render(data);\n        }\n        else\n            this.currentRoute.render();\n    }\n\n    findRoute(path){\n        //  path:   /item/1\n        //  need:   /item/:index\n\n    }\n\n    back() {\n        this.history.back();\n    }\n\n    forward() {\n        this.history.forward();\n    }\n}"},"lineCount":null}},"error":null,"hash":"0f3479a06374a0b39bc7463f133d853c","cacheData":{"env":{}}}